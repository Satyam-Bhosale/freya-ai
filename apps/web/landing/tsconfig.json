{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": [
      "ES2020",
      "DOM"
    ],
    "module": "ESNext",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@shadcn/ui/*": [
        "../../packages/ui/src/*"
      ]
    }
  }
}